{
  "name": "housefly",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\"",
    "check-types": "turbo run check-types",
    "validate": "npx tsx packages/cli/src/main.ts validate",
    "ca": "npm run validate --",
    "run-exercise": "npx tsx packages/cli/src/main.ts run",
    "watch": "npx tsx packages/cli/src/main.ts watch",
    "hint": "npx tsx packages/cli/src/main.ts hint",
    "reset": "npx tsx packages/cli/src/main.ts reset",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/pdf-parse": "^1.1.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.0",
    "tsx": "^4.0.0",
    "turbo": "^2.5.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "exercises/*"
  ],
  "dependencies": {
    "crawlee": "^3.13.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.51.1"
  }
}
