{
  "name": "housefly",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check-types": "turbo run check-types",
    "validate": "tsx packages/cli/src/main.ts validate",
    "ca": "pnpm run validate --",
    "run-exercise": "tsx packages/cli/src/main.ts run",
    "watch": "tsx packages/cli/src/main.ts watch",
    "hint": "tsx packages/cli/src/main.ts hint",
    "reset": "tsx packages/cli/src/main.ts reset",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@types/pdf-parse": "^1.1.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "tsx": "^4.0.0",
    "turbo": "^2.5.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.15.4",
  "dependencies": {
    "crawlee": "^3.13.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.51.1"
  }
}
